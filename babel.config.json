{
    "presets": ["es2015", "es2017", "stage-0"],
  
    "plugins": [
      "transform-runtime",
      "add-module-exports",
      "transform-decorators-legacy"
    ],
  
    "env": {
      "development": {
        "plugins": [
          "typecheck"
        ]
      }
    },
  
    "sourceMaps": true,
    "retainLines": true
  }
  